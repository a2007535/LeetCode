    "class Solution:\n",
    "    def longestSubsequence(self, arr: List[int], difference: int) -> int:\n",
    "        dp = {}\n",
    "        for i in arr:\n",
    "            if i-difference in dp:\n",
    "                dp[i] = dp[i-difference]+1\n",
    "            else:\n",
    "                dp[i]=1\n",
    "        return max(dp.values())"
   ]
  }
