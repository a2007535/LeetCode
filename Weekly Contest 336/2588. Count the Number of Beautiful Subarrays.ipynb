   "source": [
    "class Solution:\n",
    "    def beautifulSubarrays(self, A):\n",
    "        dp = Counter({0: 1})\n",
    "        res = pre = 0\n",
    "        for a in A:\n",
    "            pre ^= a\n",
    "            res += dp[pre]\n",
    "            dp[pre] += 1\n",
    "        return res"
   ]
  }
