    "class Solution:\n",
    "    def isPowerOfTwo(self, n: int) -> bool:\n",
    "        if n <0:\n",
    "            return False\n",
    "        s = format(n,'b')\n",
    "        s= str(s)\n",
    "        counter = Counter(s)\n",
    "        return counter['1'] == 1"
   ]
