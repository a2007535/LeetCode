    "class Solution:\n",
    "    def putMarbles(self, weights: List[int], k: int) -> int:\n",
    "        arr = []\n",
    "        for i in range(1,len(weights)):\n",
    "            arr.append(weights[i]+weights[i-1])\n",
    "        arr.sort()\n",
    "        k = k-1\n",
    "        return sum(arr[-k:])-sum(arr[:k]) if k!= 0 else 0\n",
    "    "
   ]
  }
