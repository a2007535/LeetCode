    "class Solution:\n",
    "    def findPrefixScore(self, nums: List[int]) -> List[int]:\n",
    "        ans = []\n",
    "        maxVal = 0\n",
    "        res = 0\n",
    "        for i in nums:\n",
    "            res += i\n",
    "            maxVal = max(maxVal, i)\n",
    "            res += maxVal\n",
    "            ans.append(res)\n",
    "        return ans"
   ]
  }
