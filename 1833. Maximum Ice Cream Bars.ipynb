    "class Solution:\n",
    "    def maxIceCream(self, costs: List[int], coins: int) -> int:\n",
    "        costs=sorted(costs)\n",
    "        ans=0\n",
    "        for i in costs:\n",
    "            if coins >= i:\n",
    "                coins-=i\n",
    "                ans+=1\n",
    "            else:\n",
    "                break\n",
    "        return ans"
   ]
  }
