    "class Solution:\n",
    "    def findMaxK(self, nums: List[int]) -> int:\n",
    "        ans = 0\n",
    "        tempset = set(nums)\n",
    "        for i in nums:\n",
    "            if -i in tempset:\n",
    "                ans = max (ans, abs(i))\n",
    "        return ans if ans != 0 else -1"
   ]
  }
