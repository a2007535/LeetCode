    "class Solution:\n",
    "    def sumOfNumberAndReverse(self, num: int) -> bool:\n",
    "        for i in range(0,num+1):\n",
    "            reverse = \"\".join(list(str(i))[::-1])\n",
    "            if int(reverse) + i == num:\n",
    "                return True\n",
    "        return False"
   ]
