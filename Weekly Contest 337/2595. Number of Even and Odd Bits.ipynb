    "class Solution:\n",
    "    def evenOddBit(self, n: int) -> List[int]:\n",
    "        ans = [0,0]\n",
    "        pos = 0\n",
    "        while n != 0:\n",
    "            ans[pos] += n%2\n",
    "            n = n//2\n",
    "            pos += 1\n",
    "            pos %= 2\n",
    "        return ans"
   ]
  }
