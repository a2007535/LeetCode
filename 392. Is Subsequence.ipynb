    "class Solution:\n",
    "    def isSubsequence(self, s: str, t: str) -> bool:\n",
    "        left = 0\n",
    "        if not s:\n",
    "            return True\n",
    "        for i in t:\n",
    "            if i == s[left]:\n",
    "                left+=1\n",
    "            if left == len(s):\n",
    "                return True\n",
    "        return False"
   ]
