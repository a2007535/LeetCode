    "class Solution:\n",
    "    def minFlipsMonoIncr(self, s: str) -> int:\n",
    "        best=s.count('0')\n",
    "        res=best\n",
    "        for i in s:\n",
    "            if i=='0':\n",
    "                res-=1\n",
    "            else:\n",
    "                res+=1\n",
    "            best=min(best,res)\n",
    "        return best\n",
    "    "
   ]
  }
