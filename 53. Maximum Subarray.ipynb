    "class Solution:\n",
    "    def maxSubArray(self, nums: List[int]) -> int:\n",
    "        res = ans = nums[0]\n",
    "        for i in nums[1:]:\n",
    "            if res + i >= i:\n",
    "                res = res+i\n",
    "            else:\n",
    "                res = i\n",
    "            ans = max(ans, res)\n",
    "        return ans"
   ]
