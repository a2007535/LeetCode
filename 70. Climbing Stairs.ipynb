    "class Solution:\n",
    "    def climbStairs(self, n: int) -> int:\n",
    "        arr = [1,2]\n",
    "        for _ in range(n-2):\n",
    "            arr.append(arr[-1]+arr[-2])\n",
    "        return arr[n-1]"
   ]
