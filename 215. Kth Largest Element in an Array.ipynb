    "class Solution:\n",
    "    def findKthLargest(self, nums: List[int], k: int) -> int:\n",
    "        h = []\n",
    "        for i in nums:\n",
    "            heappush(h,-i)\n",
    "        for _ in range(k):\n",
    "            ans = -heappop(h)\n",
    "        return ans"
   ]
  }
