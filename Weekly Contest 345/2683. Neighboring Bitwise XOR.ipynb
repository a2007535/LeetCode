    "class Solution:\n",
    "    def doesValidArrayExist(self, derived: List[int]) -> bool:\n",
    "        init = 1\n",
    "        for i in derived:\n",
    "            if i ==1:\n",
    "                init *= -1\n",
    "        return init == 1"
